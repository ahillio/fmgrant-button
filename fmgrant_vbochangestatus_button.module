<?php 
/**
 * Implements hook_form_alter().
 */
function fmgrant_vbochangestatus_button_form_alter(&$form, &$form_state, $form_id) {
  drupal_set_message($form_id);

  #krumo($form);
  switch ($form_id) {
    case 'edit_actionviews_bulk_operations_modify_action' :
      #krumo($form);
      $form['element']['#value'] = t('Update status of selected grants');

    break;
  }
}



